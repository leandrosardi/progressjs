<%
fname = params['first_name']
lname = params['last_name']
role = 'Unknown'

if fname == 'Clark' && lname == 'Kent'
    role = 'Superman'
elsif fname == 'Bruce' && lname == 'Wayne'
    role = 'Batman'
elsif fname == 'Peter' && lname == 'Parker'
    role = 'Spiderman'
end

ret = {}
ret[:result] = params.to_s
return ret.to_json
%>

